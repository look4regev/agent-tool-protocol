{
	"name": "@mondaydotcomorg/atp-protocol",
	"version": "0.17.16",
	"description": "Core protocol types and interfaces for Agent Tool Protocol",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsc -p tsconfig.json --watch",
		"clean": "rm -rf dist *.tsbuildinfo",
		"test": "vitest run",
		"lint": "tsc --noEmit"
	},
	"keywords": [
		"agent",
		"protocol",
		"types",
		"ai",
		"llm"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/your-org/agent-tool-protocol.git",
		"directory": "packages/protocol"
	},
	"license": "MIT",
	"dependencies": {
		"@mondaydotcomorg/atp-provenance": "0.17.16",
		"zod": "^3.23.8"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"typescript": "^5.3.3",
		"vitest": "^1.2.1"
	}
}
