{
	"name": "@agent-tool-protocol/protocol",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/protocol/src",
	"projectType": "library",
	"tags": [
		"atp-core"
	],
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{projectRoot}/dist"],
			"options": {
				"command": "tsc --build packages/protocol/tsconfig.json"
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"options": {
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "tsc -p packages/protocol/tsconfig.json --noEmit"
			}
		}
	}
}
