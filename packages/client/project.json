{
	"name": "@mondaydotcomorg/atp-client",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/client/src",
	"projectType": "library",
	"tags": ["atp-core"],
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{projectRoot}/dist"],
			"options": {
				"command": "tsc --build packages/client/tsconfig.json"
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"options": {
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "cd packages/client && npm run lint"
			}
		}
	}
}
