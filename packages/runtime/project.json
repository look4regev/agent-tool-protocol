{
	"name": "@agent-tool-protocol/runtime",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/runtime/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{projectRoot}/dist"],
			"options": {
				"command": "tsc --build packages/runtime/tsconfig.json"
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"options": {
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "cd packages/runtime && npm run lint"
			}
		}
	}
}
