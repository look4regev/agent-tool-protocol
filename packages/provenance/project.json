{
	"name": "@mondaydotcomorg/atp-provenance",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/provenance/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{workspaceRoot}/packages/provenance/dist"],
			"options": {
				"command": "tsc --build packages/provenance/tsconfig.json",
				"cwd": "{workspaceRoot}"
			}
		},
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"command": "rm -rf packages/provenance/dist"
			}
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "cd packages/provenance && npm run lint"
			}
		}
	},
	"tags": ["atp-core"]
}
